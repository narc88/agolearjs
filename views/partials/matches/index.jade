ul
	li(ng-repeat="match in matches") 
		div(class="row")
			div(class="col-md-1")
				a(href="#", class="thumbnail")
					img(src="data:image/svg+xml;base64,", style="width: 64px; height: 64px;", alt="...")
			div(class="col-md-8")
				a(href="/teams/{{match.local_team._id}}") {{match.local_team.name}} 
				span vs
				a(href="/teams/{{match.visitor_team._id}}") {{match.visitor_team.name}}
				span {{match.turn[0].soccer_field.name}}
				span {{match.turn[0].hour}}
				span {{match.turn[0].minute}}
				a(href="/matches/{{match._id}}") Ver
			div(class="col-md-1")
					a(href="#", class="thumbnail")
						img(src="data:image/svg+xml;base64,", style="width: 64px; height: 64px;", alt="...")