div(class="local col-md-6")
	div(class="col-md-2")
		a(href="#", class="thumbnail")
			img(src="data:image/svg+xml;base64,", style="width: 64px; height: 64px;", alt="...")
	div(class="col-md-9")
		p {{match.local_team.name}}
		span {{match.local_goals.length}}
