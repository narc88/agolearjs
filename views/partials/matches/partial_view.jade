	div(class="col-md-9")
		p {{match.visitor_team.name}}
		span {{match.visitor_goals.length}}
	div(class="col-md-2")
		a(href="#", class="thumbnail")
			img(src="data:image/svg+xml;base64,", style="width: 64px; height: 64px;", alt="...")